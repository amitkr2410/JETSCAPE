<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 20 </nEvents>
  
  <JetScapeWriterAscii> on </JetScapeWriterAscii>

  <!-- Inital State Module  -->
  <IS>
    <Trento> </Trento>
  </IS>
  
  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>50</pTHatMin>
      <pTHatMax>70</pTHatMax>
      <eCM>5020</eCM>
    </PythiaGun>
  </Hard>
  
  <!--Preequilibrium Dynamics Module -->
  <Preequilibrium>
    <FreestreamMilne> </FreestreamMilne>
  </Preequilibrium>
  
  <!-- Hydro  Module  -->
  <Hydro>
      <CLVisc></CLVisc>
  </Hydro>

   <!--Eloss Modules -->
  <Eloss>
    <Matter>
      <in_vac> 0 </in_vac>
    </Matter>
    <Lbt> </Lbt>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>colorless</name>
  </JetHadronization>
  
</jetscape>
